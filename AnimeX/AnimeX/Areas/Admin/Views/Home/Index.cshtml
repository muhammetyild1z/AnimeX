@model List<EntityLayer.AppUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}


    <section class="content">
       
        @await Component.InvokeAsync("_Info4Boxes")
        

        <div class="row">
            <div class="col-md-8">
                <div class="row">

                    <div class="col-md-6">
                        <!-- USERS LIST -->


                        <div class="box box-danger">
                            <div class="box-header with-border">
                                <h3 class="box-title">Son Kayıt Olan Kullanıcılar</h3>

                                <div class="box-tools pull-right">
                                    <span class="label label-danger">@Model.Count() Kayıtlı Kullanıcı</span>
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                    <button type="button" class="btn btn-box-tool" data-widget="remove">
                                        <i class="fa fa-times"></i>
                                    </button>
                                </div>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body no-padding">
                                <ul class="users-list clearfix">
                                    @foreach (var item in Model)
                                    {
                                        <li>
                                            <img src="https://secure.gravatar.com/avatar/@item.EmailHash?s=200" alt="User Image">
                                            <a class="users-list-name" href="/Member/Memberdetails/@item.Id">@item.UserName</a>
                                            <span class="users-list-date">@item.UserCreateDate.ToString("dd MMMM yyyy")</span>
                                        </li>
                                    }


                                </ul>
                                <!-- /.users-list -->
                            </div>
                            <!-- /.box-body -->
                            <div class="box-footer text-center">
                                <a href="/Admin/Home/AllUsers" class="uppercase">Tüm Kullanıcıları Getir</a>
                            </div>
                            <!-- /.box-footer -->
                        </div>
                        <!--/.box -->
                    </div>

                </div>


                @await Component.InvokeAsync("_FeedbackMembers")
       
            </div>
            
            @await Component.InvokeAsync("_AdminProfile")
          
        </div>      
    </section>  

